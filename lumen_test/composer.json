{
    "name": "laravel/lumen",
    "description": "The Laravel Lumen Framework.",
    "keywords": ["framework", "laravel", "lumen"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": "^7.4|^8.0",
        "laravel/lumen-framework": "^8.0",
        "ext-http": "*"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "mockery/mockery": "^1.3.1",
        "phpunit/phpunit": "^9.3"
    },
    "autoload": {
        "psr-4": {
            "App\\": "external/Framework/Lumen/app/",
            "Database\\Factories\\": "external/Framework/Lumen/database/factories/",
            "Database\\Seeders\\": "external/Framework/Lumen/database/seeders/",
            "Domain\\": "domain/",
            "Usecase\\": "usecase/",
            "Adapter\\": "adapter/",
            "External\\": "external/"
        }
    },
    "autoload-dev": {
        "classmap": [
            "external/Framework/Lumen/tests/"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
